                <div class="modal fade" id="wishlistModal" tabindex="-1" aria-labelledby="wishlistModalLabel" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content" style="background-color: rgb(5, 5, 4); width: 60%; margin-left: 21%;height: 55px; ">
                            <div class="modal-body" id="wishlistModalBody" style="margin: auto; color: rgb(255, 255, 255);">
                                <!-- Wishlist status message will be displayed here -->
                            </div>
                        </div>
                    </div>
                </div>
                <script>
                    function toggleWishlist(productId) {
                        axios.post(`/wishlist/${productId}`)
                            .then(function (response) {
                                console.log(response.data);
                                // Show wishlist status message
                                showWishlistStatusMessage(response.data.message);
            
                                // Update heart icon color based on response
                                const heartIcon = document.querySelector(`#heartIcon_${productId}`);
                                if (response.data.color === true) {
                                    heartIcon.classList.remove('text-dark');
                                    heartIcon.classList.add('text-danger'); // Change to red color
                                } else {
                                    heartIcon.classList.remove('text-danger'); // Change to normal color
                                    heartIcon.classList.add('text-dark');
                                }
                            })
                            .catch(function (error) {
                                console.error('Error:', error);
                            });
                    }
            
                    // Function to display wishlist status message
                    function showWishlistStatusMessage(message) {
                        Toastify({
                            text: message,
                            duration: 2000, // 2000 milliseconds = 2 seconds
                            gravity: 'top', // Position of the toast message
                            position: 'right', // Alignment of the toast message
                            backgroundColor:'green'
                        }).showToast();
                    }
            
                    // Function to add product to cart
                    function addtoCart(productId) {
                        axios.post(`/cart/${productId}`)
                            .then(function (response) {
                                console.log(response.data.message);
                                if (response.data.cartcount) {
                                    document.getElementById("cartcount").innerText = ""
                                    document.getElementById("cartcount").innerHTML = response.data.cartcount
                                }
                                if (response.data.message) {
                                    showWishlistStatusMessage(response.data.message);
                                }
                            })
                            .catch(function (error) {
                                console.error('Error:', error);
                            });
                    }
                </script>

<script src="https://cdn.jsdelivr.net/npm/toastify-js@1.12.0/src/toastify.min.js"></script>





                <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
                <script>


                

                    function submitCategory(category) {
                        console.log("add triggered:", category);
                        axios.post('/latestproduct', {
                            category: category
                        })
                        .then(function(response) {
                            console.log(response.data);

                            const products = response.data.products;
        
                            const container = document.getElementById('productsContainer');
            
                            // Clear previous content
                            container.innerHTML = '';
            
                            // Iterate over products and create HTML elements dynamically
                            products.forEach(product => {
                                const productHTML = `
                                    <div class="col-md-6 col-lg-4 col-xl-3">
                                        <div class="rounded position-relative fruite-item">
                                            <div class="fruite-img position-relative">
                                                <a href="/productveiw/${product._id}"><img src="${product.image[0]}" class="img-fluid w-100 rounded-top" alt=""></a>
                                    
                                                <!-- End Heart icon -->
                                            </div>
                                            <div class="text-white bg-secondary px-3 py-1 rounded position-absolute" style="top: 10px; left: 10px;">${product.category}</div>
                                            <div class="p-4 border border-secondary border-top-0 rounded-bottom">
                                                <h4>${product.productName}</h4>
                                               
                                                <div class="">
                                                    <p class="text-dark fs-5 fw-bold mb-0 mt-1">₹${product.productPrice}</p><br>
                                                    <button type="submit" class="btn border border-secondary rounded-pill px-3 text-primary addToCartBtn" onclick="addtoCart('${product._id}')">
                                                    <i class="fa fa-shopping-bag me-2 text-primary"></i> Add to cart
                                                </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                `;
            
                                container.innerHTML += productHTML;
                            });
                        })
                        .catch(function(error) {
                            console.error('Error adding product to cart:', error);
                            // Optionally, show an error message to the user
                        });
                    }

                    // setInitialHeartIconState();
                </script>
                            </div>
